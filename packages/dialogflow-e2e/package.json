{
    "name": "e2e",
    "version": "1.0.0",
    "scripts": {
        "e2e": "cucumber-js --require-module ts-node/register features/**/*.feature --require \"step/**/!(*.spec).ts\" --tags \"not @skip\"",
        "tag": "npm run e2e -- -f node_modules/cucumber-pretty --tags",
        "prettier": "prettier --write \"step/**/*.ts\"",
        "tdd": "mocha --recursive --watch --require ts-node/register \"./step/**/*.spec.ts\"",
        "test": "mocha --recursive --require ts-node/register \"./step/**/*.spec.ts\""
    },
    "dependencies": {
        "botium-connector-dialogflow": "0.0.8",
        "botium-connector-google-assistant": "0.0.2",
        "botium-core": "^1.5.1",
        "cucumber": "^5.1.0",
        "cucumber-pretty": "^1.5.2",
        "dotenv": "^8.0.0",
        "firebase-admin": "^8.2.0",
        "lodash": "^4.17.15",
        "momentjs": "2.0.0"
    },
    "devDependencies": {
        "@google-cloud/firestore": "^2.2.4",
        "@types/chai": "4.1.7",
        "@types/cucumber": "4.0.7",
        "@types/dotenv": "^6.1.1",
        "@types/lodash": "^4.14.136",
        "@types/mocha": "5.2.7",
        "@types/node": "12.6.6",
        "mocha": "6.1.4",
        "prettier": "1.18.2",
        "sinon": "7.3.2",
        "ts-node": "8.3.0",
        "tslint": "5.18.0",
        "tslint-config-prettier": "1.18.0",
        "typescript": "^3.5.3"
    }
}
